                <API Name="getOrderFulfillmentDetails">
                        <Input>
                <Order OrderHeaderKey="" DisplayLocalizedFieldInLocale="xml:CurrentUser:/User/@Localecode"/>
                        </Input>
                        <Template>
              <Order OrderHeaderKey="" >
                                 <OrderLines>
                                        <OrderLine DeliveryMethod="" MinLineStatus="" MaxLineStatus="" OrderLineKey="" OrderedQty="" ItemID=""  HasAnyUnavailableQty="">
                                                <Item ItemID="" UnitOfMeasure="" ProductClass="" />
